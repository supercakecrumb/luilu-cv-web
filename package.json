{
  "name": "luilu-cv-web",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Portfolio website for graphic/UX designer Luiza",
  "author": "Luiza",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
